{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ziorwebdev/icon",
	"title": "Icon",
	"category": "widgets",
	"description": "Display an icon.",
	"textdomain": "default",
	"parent": [
		"ziorwebdev/icon-picker"
	],
	"attributes": {
		"url": {
			"type": "string",
			"role": "content"
		},
		"service": {
			"type": "string"
		},
		"label": {
			"type": "string",
			"role": "content"
		},
		"rel": {
			"type": "string"
		}
	},
	"usesContext": [
		"openInNewTab",
		"showLabels",
		"iconColor",
		"iconColorValue",
		"iconBackgroundColor",
		"iconBackgroundColorValue"
	],
	"supports": {
		"reusable": false,
		"html": false,
		"interactivity": {
			"clientNavigation": true
		}
	},
	"editorStyle": "wp-block-ziorwebdev-icon-editor"
}